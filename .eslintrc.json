{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:vue/essential", "airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "linebreak-style": 0,
    "no-console": "off",
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "import/extensions": [
      "error",
      "always",
      {
        "js": "never",
        "vue": "never"
      }
    ]
  }
}
